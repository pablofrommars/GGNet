@typeparam T
@typeparam TX
@typeparam TY

@inherits PlotBase<T, TX, TY>

<svg id=@Id class="ggnet" theme="default" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" viewBox="0 0 @Width @Height">
    <defs>
        <clipPath id=@(Id + "-" + Panel.Id + "-area")>
            <rect x=@Area.X y=@Area.Y width=@Area.Width height=@Area.Height />
        </clipPath>
    </defs>

    <rect class="panel" x=@Area.X y=@Area.Y width=@Area.Width height=@Area.Height />

    <Area T=T TX=TX TY=TY Panel=@Panel Coord=@this Zone=@Area Clip=@(Id + "-" + Panel.Id + "-area") RenderPolicy=@renderChildPolicy />

    <SparkLineTooltip Coord=@this Zone=@Area @ref=@tooltip />
</svg>
