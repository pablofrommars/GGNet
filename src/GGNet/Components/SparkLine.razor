@typeparam T
@typeparam TX
@typeparam TY

@inherits PlotBase<T, TX, TY>

<svg id=@Id xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" viewBox="0 0 @Width @Height">
    <GGNet.Components.Definitions.SparkLine Id=@Id Theme=@Theme />

    <defs>
        <clipPath id=@(Id + "-" + Panel.Id + "-area")>
            <rect x=@Area.X y=@Area.Y width=@Area.Width height=@Area.Height />
        </clipPath>
    </defs>

    <GGNet.Components.Definitions.Animation Id=@Id Theme=@Theme />

    <rect x=@Area.X y=@Area.Y width=@Area.Width height=@Area.Height class="panel" />

    <Area T=T TX=TX TY=TY Panel=@Panel Coord=@this Zone=@Area Clip=@(Id + "-" + Panel.Id + "-area") RenderPolicy=@renderChildPolicy />

    <GGNet.Components.Tooltips.SparkLine Id=@Id Coord=@this Area=@Area Theme=@Theme @ref=@tooltip />
</svg>